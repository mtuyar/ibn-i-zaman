{
  "expo": {
    "name": "Zamanın Oğlu",
    "slug": "ibn-i-zaman",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon_new.png",
    "scheme": "ibnizaman",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/icon_new.png",
      "buildNumber": "3",
      "bundleIdentifier": "com.mtuyarr.ibnizaman",
      "bitcode": false,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "name": "Zamanın Oğlu",
      "icon": "./assets/images/icon_new.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon_new.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "notification": {
        "icon": "./assets/images/icon_new.png"
      },
      "package": "com.mtuyarr.ibnizaman",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/images/icon.png",
            "backgroundColor": "#0F172A"
          }
        }
      ],
      "expo-font",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/favicon.png",
          "color": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d3bc9429-a22c-4bc9-a1d7-45336002f49a"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}